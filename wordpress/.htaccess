
# BEGIN WordPress
<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /Proyectos/Mova/MovaWebPage/wordpress/
RewriteRule ^index\.php$ - [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /Proyectos/Mova/MovaWebPage/wordpress/index.php [L]
</IfModule>

# END WordPress